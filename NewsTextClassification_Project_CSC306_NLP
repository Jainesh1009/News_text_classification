          # removes all the unwanted spaces\n","    sentence = sentence.lower()                        # converts all text in the senctence into lowercase\n","    return sentence                                    # returning the sentence\n","\n","\n","def data_cleaning(data):                               # defining a function for data cleaning\n","    data['Cleaned']=None                               # adding a new coloumn to the csv file named 'Cleaned'\n","    for index, row in data.iterrows():                 # running a for loop to add the cleaned data to the new coloumn\n","        data.loc[index,'Cleaned']=sentence_cleaning(row[\"text\"])     # User-defined function 'sentence_cleaning' is used to clean the content\n","        # data.loc is used to access a group of rows & columns by label\n","    print('Data Cleaned')\n","    return data\n","\n","# category       text\n","# sports         Badminton is a sports.\n","# returned data...\n","# category       text                        Cleaned\n","# sports         Badminton is a sports.      Badminton, sports"],"execution_count":8,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"loJRw5mc7nx6","executionInfo":{"status":"ok","timestamp":1620218098649,"user_tz":-330,"elapsed":3427,"user":{"displayName":"Yesha Ajudia","photoUrl":"","userId":"06177778231505819887"}},"outputId":"fa69119d-6c64-42f7-ddfc-59dff7da4df5"},"source":["# we need index nunmber in the data_cleaning function\n","data=data_cleaning(data)            # running the data_cleaning function on the given data"],"execution_count":9,"outputs":[{"output_type":"stream","text":["Data Cleaned\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"id":"O7-IrErA7pve","colab":{"base_uri":"https://localhost:8080/","height":279},"executionInfo":{"status":"ok","timestamp":1620218098651,"user_tz":-330,"elapsed":3013,"user":{"displayName":"Yesha Ajudia","photoUrl":"","userId":"06177778231505819887"}},"outputId":"d71